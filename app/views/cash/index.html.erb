<% if current_user.admin? %>
<div class="span12">
  <ul class="years">
    <% @m.each do |year| %>
        <li><%= link_to_unless_current year, plan_url(:year => year)%></li>
    <% end %>
  </ul>
  <br/>
  <br/>
  <h3>Данные по банкам и по кассе - <%= Russian::strftime(@cash.first.DAT, "%d %B %Y") %></h3>
  <table class="table table-hover">
    <tr>
      <th>Наименование</th>
      <th>Сбербанк</th>
      <th>Россельхоз банк</th>
    </tr>
    <%= render :partial => "cash/cash_item", :collection => @cash, :as => :item  %>
  </table>
</div>
<% else %>
    <h2>Недостаточно прав</h2>
<% end %>